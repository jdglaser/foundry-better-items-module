{
  "id": "dnd5e-item-improvements",
  "title": "D&D5e Item Improvements",
  "description": "Enhance item information and slot based encumberance system.",
  "version": "1.0.0",
  "compatibility": { "minimum": "13", "verified": "13" },
  "authors": [{ "name": "Jarred Glaser" }],
  "esmodules": ["module.js"],
  "packs": [
    {
      "name": "rules-reference",
      "label": "Rules Reference",
      "path": "packs/rules-reference.db",
      "type": "JournalEntry",
      "system": "dnd5e"
    },
    {
      "name": "items",
      "label": "Items",
      "path": "packs/items.db",
      "type": "Item",
      "system": "dnd5e"
    }
  ],
  "packFolders": [
    {
      "name": "D&D 5e Better Items",
      "color": "#007BAC",
      "sorting": "m",
      "packs": ["rules-reference", "items"]
    }
  ],
  "relationships": {
    "requires": [
      {
        "id": "lib-wrapper",
        "type": "module"
      }
    ]
  },
  "styles": ["styles.css"]
}
